{
    "presets": [
        ["env", {
            "targets": {
                "node": true,
                "browsers": [
                    "last 2 Chrome versions"
                ]
            },
            "loose": true,
            "modules": false
        }],
        "react"
    ],
    "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread",
        "syntax-dynamic-import"
    ]
}
