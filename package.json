{
    "name": "js-performance",
    "version": "0.0.1",
    "decription": "A satellite repository for JavaScript Performance talk @NI Code Naturally",
    "license": "MIT",
    "scripts": {
        "clean": "rimraf dist/",
        "copy": "ncp src/config/ dist/config/",
        "mkdir": "mkdirp dist/config/",
        "build:app": "webpack",
        "build:app:prod": "webpack -p --env.production",
        "watch:app": "webpack -w",
        "build": "run-s clean mkdir copy reload build:app",
        "build:prod": "run-s clean mkdir copy reload build:app:prod",
        "watch": "run-s clean mkdir copy reload watch:app",
        "start": "docker-compose up -d --build",
        "stop": "docker-compose down",
        "reload": "docker restart js-perf"
    },
    "dependencies": {
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-router": "^4.1.1",
        "react-router-dom": "^4.1.1",
        "semantic-ui-react": "^0.71.0"
    },
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.1.1",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.24.1",
        "eslint": "3.19.0",
        "eslint-config-airbnb": "^15.0.2",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "^7.1.0",
        "html-webpack-plugin": "^2.29.0",
        "mkdirp": "^0.5.1",
        "ncp": "^2.0.0",
        "npm-run-all": "^4.0.2",
        "optimize-js-plugin": "^0.0.4",
        "prettier": "^1.5.2",
        "webpack": "^3.1.0",
        "webpack-merge": "^4.1.0"
    }
}
