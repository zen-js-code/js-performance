{
    "name": "js-performance",
    "version": "0.0.1",
    "decription": "An examples repository for JavaScript Performance talk @Code Naturally",
    "license": "MIT",
    "scripts": {
        "lint": "eslint src/ examples/",
        "load": "loadtest -n 100 -c 10 --rps 10 http://localhost:5151/api/users",
        "profile1": "webpack --profile --json > stats.json",
        "profile2": "webpack --display-optimization-bailout",
        "watch": "gulp watch",
        "build": "gulp build",
        "build:prod": "gulp build",
        "server": "node --inspect --harmony dist/server/",
        "d:up": "docker-compose up -d --build",
        "d:down": "docker-compose down",
        "d:reload": "run-s d:down d:up",
        "start": "run-s d:down build d:up server",
        "start:prod": "run-s d:down build:prod d:up server"
    },
    "dependencies": {
        "dummy-json": "^2.0.0",
        "express": "^4.15.3",
        "lodash": "^4.17.4",
        "morgan": "^1.8.2",
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "react-dom": "^15.6.1",
        "react-redux": "^5.0.5",
        "react-router": "^4.1.2",
        "react-router-dom": "^4.1.2",
        "redux": "^3.7.2",
        "semantic-ui-react": "^0.71.1"
    },
    "devDependencies": {
        "babel-core": "^6.25.0",
        "babel-eslint": "^7.2.3",
        "babel-loader": "^7.1.1",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.24.1",
        "del": "^3.0.0",
        "eslint": "3.19.0",
        "eslint-config-airbnb": "^15.0.2",
        "eslint-loader": "^1.9.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "5.1.1",
        "eslint-plugin-react": "^7.1.0",
        "gulp": "gulpjs/gulp#4.0",
        "gulp-nodemon": "^2.2.1",
        "html-webpack-plugin": "^2.29.0",
        "loadtest": "^2.3.0",
        "nodemon": "^1.11.0",
        "npm-run-all": "^4.0.2",
        "optimize-js-plugin": "^0.0.4",
        "uglify-es": "^3.0.25",
        "uglifyjs-webpack-plugin": "^1.0.0-beta.1",
        "v8-compile-cache": "^1.1.0",
        "webpack": "^3.3.0",
        "webpack-merge": "^4.1.0"
    }
}
